page {
  height: 100%;
}

.advice-container {
  display: flex;
  flex-direction: column;
  height: 100%;
  background-color: #F5F5F5;
  align-items: center;
  justify-content: space-between;
}

.logo {
  width: 360px;
  height: 111px;
}

.contact {
  display: flex;
  width: 80%;
  flex-direction: column;
  justify-content: space-between;
  height: 180px;
  margin-left: 30px;
}

.contact-text{
  font-family: SimSun;
  font-size: 20px;
  line-height: 28px;
  font-weight: 600;
}
.bottom {
  width: 239px;
  height: 181px;
}