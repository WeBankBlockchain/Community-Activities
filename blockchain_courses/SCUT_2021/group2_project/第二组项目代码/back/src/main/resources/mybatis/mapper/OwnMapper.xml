<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fisco.app.mapper.OwnMapper">


    <insert id="addOwn">
        insert into own(user_id,cert_id,content,timestamp) values (
        #{userId}, #{certId}, #{content}, #{time})
    </insert>

</mapper>